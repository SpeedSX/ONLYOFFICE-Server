      <add name="AjaxPro" verb="POST,GET" path="ajaxpro/*.ashx" type="AjaxPro.AjaxHandlerFactory, AjaxPro.2" />
      <add name="Jwt" verb="POST,GET" path="jwtlogin.ashx" type="ASC.Web.Studio.HttpHandlers.JwtHandler, ASC.Web.Studio" />
